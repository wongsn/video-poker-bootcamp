<!-- Scorechart -->
<div class="scorechart" id="scorechart">
  <div id="chartcontent">
    <table>
      <thead>
        <tr>
          <th><h2>Hand</h2></th>
        </tr>
      </thead>
      <tbody id="chartbody">
        <tr>
          <td id="royalflush">
            <span id="specialhands">🂪 🂫 🂭 🂮 🂡</span><br />Royal Flush | 250 x
          </td>
        </tr>
        <tr>
          <td id="straightflush">
            <span id="specialhands">🃂 🃋 🃄 🃆 🃊</span><br />Straight Flush | 50 x
          </td>
        </tr>
        <tr>
          <td id="fourofakind">
            <span id="specialhands">🂡 🂡 🂡 🂡 🃟</span><br />Four of a kind | 25 x
          </td>
        </tr>
        <tr>
          <td id="fullhouse">
            <span id="specialhands">🃞 🂾 🂮 🃁 🃑</span><br />Full House | 9 x
          </td>
        </tr>
        <tr>
          <td id="flush">
            <span id="specialhands">🂳 🂶 🂻 🂴 🂺</span><br />Flush | 6 x
          </td>
        </tr>
        <tr>
          <td id="straight">
            <span id="specialhands">🂳 🃔 🃕 🃖 🃗</span><br />Straight | 4 x
          </td>
        </tr>
        <tr>
          <td id="threeofakind">
            <span id="specialhands">🃇 🃗 🂷 🃟 🃟</span><br />Three of a Kind | 3 x
          </td>
        </tr>
        <tr>
          <td id="twopairs">
            <span id="specialhands">🂶 🂦 🃟 🃇 🃗</span><br />Two Pairs | 2 x
          </td>
        </tr>
        <tr>
          <td id="jacksorbetter">
            <span id="specialhands">🃟 🃋 🃛 🃟 🃟</span><br />Jacks or Better | 1 x
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
